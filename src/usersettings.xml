      <autocentering>
         <ctl_activate>
            <name>Resize activation</name>
            <visible>1</visible>
            <desc>single</desc>
            <values>
               <item>on</item>
               <item>off</item>
            </values>
            <actived>[1 0]</actived>
         </ctl_activate>
      </autocentering>
   </windows>
   <licence>
      <NDA>
         <ctl_activate>
            <name>Non Disclosing Agreement (NDA)</name>
            <visible>1</visible>
            <desc>single</desc>
            <values>
               <item>on</item>
               <item>off</item>
            </values>
            <actived>[1 0]</actived>
         </ctl_activate>
      </NDA>
   </licence>
